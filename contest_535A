#include <iostream>
using namespace std;
int main()
{
	int s,a,b;
	cin>>s;
	if(s==0)
	{
		cout<<"zero";
	}
	else
	{
		a=s/10;
		b=s%10;
		if(a==1)
		{
			if(b==0)
			{
				cout<<"ten";
			}
			if(b==1)
			{
				cout<<"eleven";
			}
			if(b==2)
			{
				cout<<"twelve";
			}
			if(b==3)
			{
				cout<<"thirteen";
			}
			if(b==4)
			{
				cout<<"fourteen";
			}
			if(b==5)
			{
				cout<<"fifteen";
			}
			if(b==6)
			{
				cout<<"sixteen";
			}
			if(b==7)
			{
				cout<<"seventeen";
			}
			if(b==8)
			{
				cout<<"eighteen";
			}
			if(b==9)
			{
				cout<<"nineteen";
			}
		}
		else
		{
			if(a==2)
			{
				cout<<"twenty";
			}
			if(a==3)
			{
				cout<<"thirty";
			}
			if(a==4)
			{
				cout<<"forty";
			}
			if(a==5)
			{
				cout<<"fifty";
			}
			if(a==6)
			{
				cout<<"sixty";
			}
			if(a==7)
			{
				cout<<"seventy";
			}
			if(a==8)
			{
				cout<<"eighty";
			}
			if(a==9)
			{
				cout<<"ninety";
			}
			if(a>=2&b!=0)
			{
				cout<<"-";
			}
			if(b==1)
			{
				cout<<"one";
			}
			if(b==2)
			{
				cout<<"two";
			}
			if(b==3)
			{
				cout<<"three";
			}
			if(b==4)
			{
				cout<<"four";
			}
			if(b==5)
			{
				cout<<"five";
			}
			if(b==6)
			{
				cout<<"six";
			}
			if(b==7)
			{
				cout<<"seven";
			}
			if(b==8)
			{
				cout<<"eight";
			}
			if(b==9)
			{
				cout<<"nine";
			}
		}
	}
	return 0;
}
